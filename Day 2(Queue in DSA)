#include <stdio.h>
#include <conio.h>
#define MAXSIZE10
int Queue[MAXSIZE];
int front = 0;
int rear = -1;

int main() {
 int choice();
 clrscr();
 
 do{
     printf("\n1. Insert\n2. Delete\n3. Display\n4. Exit\n");
     printf("Enter your choice:");
     scanf("%d", &choice);
     
     switch(choice){
         case 1:
         Insert(); break;
         case 2:
         Delete(); break;
         case 3:
         Display(); break;
     }
 }

    while(choice!=4);
    getch();
}

Insert(){
    int n;
       if(rear == MAXSIZE-1){
           printf("Queue Overflow\n"); // check if the queue is full (overflow condition)
       }
       else{
           printf("Enter an element");
           scanf("%d", &n);
           rear++; // increment the rear index and add the element
           Queue[rear] = n;
       }
}
Delete(){
    int n;
    if(front>rear){
        printf("Queue underflow\n"); // check if the queue is empty (underflow condition)
    }
    else{
        n= Queue[front];
        front++; // increment the front index
        
        printf("delete element is %d",n); // remove and return the front element
    }
    
}
void Display() {
    if (front > rear) {
        printf("Queue is empty\n"); // check if the queue is empty
    } else {
        printf("Queue elements are:\n ");
        for (int i = front; i <= rear; i++) {
            printf("%d ", Queue[i]); // display all elements in the queue
        }
        printf("\n");
    }
}
